#pragma kernel CSMain
 
RWTexture2D<float4> tex;
 
float w, h;
 
[numthreads(8,8,1)]
void CSMain (uint2 id : SV_DispatchThreadID)
{
	float w, h;
    tex.GetDimensions(w, h);
 
    float2 uv = float2(id.x/w, id.y/h);
 
    tex[id] = float4(uv, 0.0, 1.0);

    tex[id] = float4(id.x / w, id.y / h,  0.0, 1.0);
}